{
  "name": "scad",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/anoblet/scad#readme",
  "bugs": {
    "url": "https://github.com/anoblet/scad/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anoblet/scad.git"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "commit": "npm run git:commit",
    "git:commit": "wireit",
    "git:push": "wireit",
    "scad:check": "node scripts/scad-export.mjs check",
    "scad:inventory": "node scripts/scad-inventory.mjs",
    "scad:release": "node scripts/scad-export.mjs release",
    "scad:validate": "node scripts/scad-export.mjs validate",
    "push": "npm run git:push"
  },
  "devDependencies": {
    "sort-package-json": "^2.4.1",
    "wireit": "^0.9.5"
  },
  "wireit": {
    "git:commit": {
      "command": "git add . && git commit --allow-empty-message -m \"\"",
      "dependencies": [
        "sort-package-json"
      ]
    },
    "git:push": {
      "command": "git push",
      "dependencies": [
        "git:commit"
      ]
    },
    "sort-package-json": {
      "command": "sort-package-json",
      "files": [
        "**/package.json"
      ]
    }
  }
}
